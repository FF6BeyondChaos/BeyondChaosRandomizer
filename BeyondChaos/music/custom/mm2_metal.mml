#def d= v64
#def s= v64
#def e= v64
#def b= v80
#def x= v64
#def t= v48

#drum "k"= 6d |8 v80 p60 %e0
#drum "s"= 6d |9 v96 p60 %y2%s3 %e0
#drum "o"= 6d |A v40 p68 %y0%s0
#drum "h"= 6d |A v40 p68 %y5%s0
#drum "i"= 6d |A v40 p68 %y6%s0
#drum "y"= 4a |C v72 p127 %y20
#drum "z"= 4a |C v72 p0 %y20
#drum "t"= 5g+ |B v64 p90
#drum "u"= 5d+ |B v64 p65
#drum "v"= 4a+ |B v64 p40

#cdef ( %l1
#cdef ) %l0

